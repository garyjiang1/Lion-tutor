<div class="container mt-5 text-center">
  <h1>Full Calendar</h1>
  <%= month_calendar(events: @meetings) do |date, meetings| %>
    <div class="day">
      <%= date.day %>
      <% meetings.each do |meeting| %>
        <div class="card-header">
            <%= link_to meeting.title, meeting %>
        </div>
    </div>

